import org.openqa.selenium.By;
import org.openqa.selenium.WebDriver;
import org.openqa.selenium.chrome.ChromeDriver;

public class SalesForceE2E {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		System.setProperty("webdriver.chrome.driver","C:\\Users\\Swapn\\Documents\\chromedriver.exe");
		WebDriver driver = new ChromeDriver();
		driver.get("https://www.rediff.com/");
		
		driver.findElement(By.cssSelector("a[class*='signin']")).click();
		driver.findElement(By.xpath("//input[@name='login']")).sendKeys("swapnil");
		driver.findElement(By.cssSelector("input#password")).sendKeys("yogi");
		driver.findElement(By.xpath("//input[contains(@name,'procee')]")).click();
		
	}

}
