import org.openqa.selenium.By;
import org.openqa.selenium.WebDriver;
import org.openqa.selenium.chrome.ChromeDriver;

public class Facebook {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		System.setProperty("webdriver.chrome.driver","C:\\Users\\Swapn\\Documents\\chromedriver.exe");
		WebDriver driver = new ChromeDriver();
		driver.get("http://facebook.com");//open URL of the browser
		//driver.findElement(By.id("email")).sendKeys("email address");// locate by id
		//driver.findElement(By.name("pass")).sendKeys("123345");// locate by name
		//driver.findElement(By.linkText("Forgot Password?")).click();//locate by linktext
		
		driver.findElement(By.cssSelector("#email")).sendKeys("Email Address");
		driver.findElement(By.xpath("//*[@id='pass']")).sendKeys("password");
		driver.findElement(By.xpath("//*[@id='u_0_e_+M']/div[3]/a")).click();
		
		
	}

}
